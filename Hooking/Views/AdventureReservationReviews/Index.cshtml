@model IEnumerable<Hooking.Models.AdventureReservationReview>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AdventureReservationId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Review)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DidntShow)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReceivedPenalty)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AdventureReservationId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Review)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DidntShow)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReceivedPenalty)
            </td>
        </tr>
}
    </tbody>
</table>
